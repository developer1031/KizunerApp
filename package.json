{
  "name": "kizuner",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:release": "react-native run-ios --scheme 'Kizuner-release'",
    "start": "react-native start",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "codepush:ios": "appcenter codepush release-react -a InApps/Kizuner-iOS -d Production",
    "codepush:android": "appcenter codepush release-react -a InApps/Kizuner-Android -d Production",
    "codepush": "yarn codepush:ios && yarn codepush:android",
    "test": "jest",
    "lint": "eslint .",
    "restart": "rm -rf node_modules/ && npm cache verify && npm install && npm start -- --reset-cache",
    "rs": " rm -rf node_modules yarn.lock && watchman watch-del-all && yarn && cd ios && rm -rf Pods Podfile.lock && pod install && cd .. ",
    "ios:dev": "react-native run-ios --scheme 'Development'",
    "ios:dev-release": "react-native run-ios --configuration Release --scheme 'Development'",
    "ios:prod": "react-native run-ios --scheme 'Production'",
    "ios:prod-release": "react-native run-ios --configuration Release --scheme 'Production'",
    "ios:staging": "react-native run-ios --scheme 'Staging'",
    "ios:staging-release": "react-native run-ios --configuration Release --scheme 'Staging'"
  },
